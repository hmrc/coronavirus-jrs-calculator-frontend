#!/bin/bash

echo ""
echo "Applying migration RootPage"

echo "Adding routes to conf/app.routes"
echo "" >> ../conf/app.routes
echo "GET        /rootPage                       controllers.RootPageController.onPageLoad()" >> ../conf/app.routes

echo "Adding messages to conf.messages"
echo "" >> ../conf/messages.en
echo "rootPage.title = rootPage" >> ../conf/messages.en
echo "rootPage.heading = rootPage" >> ../conf/messages.en

echo "Migration RootPage completed"
